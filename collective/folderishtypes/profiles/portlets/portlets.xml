<?xml version="1.0" encoding="UTF-8"?>
<portlets
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  i18n:domain="plone">

  <assignment name="folderishtypes-listing"
      key="Folderish Document" category="content_type" visible="True"
      manager="plone.leftcolumn" type="collective.folderishtypes.listing"/>
  <assignment name="folderishtypes-listing"
      key="Folderish Event" category="content_type" visible="True"
      manager="plone.leftcolumn" type="collective.folderishtypes.listing"/>
  <assignment name="folderishtypes-listing"
      key="Folderish News Item" category="content_type" visible="True"
      manager="plone.leftcolumn" type="collective.folderishtypes.listing"/>

  <assignment name="contextual_contents"
      type="collective.folderishtypes.contextual_contents"
      category="content_type"
      key="Folderish Event"
      manager="plone.rightcolumn"
      visible="True">
    <property name="name">Contextual Contents</property>
    <property name="allowed_types">
      <element>Image</element>
      <element>File</element>
      <element>Link</element>
    </property>
  </assignment>
  <assignment name="contextual_contents"
      type="collective.folderishtypes.contextual_contents"
      category="content_type"
      key="Folderish Document"
      manager="plone.rightcolumn"
      visible="True">
    <property name="name">Contextual Contents</property>
    <property name="allowed_types">
      <element>Image</element>
      <element>File</element>
      <element>Link</element>
    </property>
  </assignment>
  <assignment name="contextual_contents"
      type="collective.folderishtypes.contextual_contents"
      category="content_type"
      key="Folderish News Item"
      manager="plone.rightcolumn"
      visible="True">
    <property name="name">Contextual Contents</property>
    <property name="allowed_types">
      <element>Image</element>
      <element>File</element>
      <element>Link</element>
    </property>
  </assignment>
  
</portlets>
