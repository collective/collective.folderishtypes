<div class="contentsListing" tal:condition="view/available">
    <metal:listingmacro define-macro="listing">
		<tal:documentcontents define="documentContents context/@@folderListing;
                                      batch documentContents;">

            <tal:listing condition="documentContents">
                <metal:listing metal:use-macro="here/folder_listing/macros/listing">
					<metal:empty metal:fill-slot="no_items_in_listing"></metal:empty>
				</metal:listing>
			</tal:listing>
		</tal:documentcontents>
	</metal:listingmacro>
</div>